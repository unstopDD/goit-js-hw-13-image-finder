{{#each this}}
<li class="gallery-item">
   <div class="photo-card">
  <img class="photo-image" src="{{webformatURL}}" alt="{{tags}}" data-source="{{largeImageURL}}" />

  <div class="stats">
    <p class="stats-item">
      <i class="material-icons">thumb_up{{likes}}</i>
      1108
    </p>
    <p class="stats-item">
      <i class="material-icons">visibility{{views}}</i>
      320321
    </p>
    <p class="stats-item">
      <i class="material-icons">comment{{comments}}</i>
      129
    </p>
    <p class="stats-item">
      <i class="material-icons">cloud_download{{downloads}}</i>
      176019
    </p>
  </div>
</div>
</li>
 
{{/each}}
